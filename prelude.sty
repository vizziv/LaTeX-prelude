  \end{genericobservation*}\ignorespacesafterend}

% Solutions to exercises.

\newenvironment{solution}[1][]{%
  \begin{proof}[Solution\ifempty{#1}{}{ to \fref{#1}}]
  \noqed}{
  \end{proof}\ignorespacesafterend}


