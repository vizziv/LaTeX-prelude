% Named theorems and observations.
\newenvironment{namedtheorem*}[1]{%
  \def\@theoremname{#1}\begin{generictheorem*}}{%
  \end{generictheorem*}\ignorespacesafterend}
\newenvironment{namedobservation*}[1]{%
